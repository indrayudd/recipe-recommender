{
    "routes": [
      {
        "src": "/(.*)",
        "has": [
          {
            "type": "header",
            "key": "user-agent",
            "value": ".*(Mobile|Android|iPhone|iPad).*"
          }
        ],
        "dest": "/build-mobile/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build-desktop/$1"
      }
    ]
  }
  