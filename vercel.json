{
    "rewrites": [
      {
        
        "source": "/(.*)",
        "has": [
          {
            "type": "header",
            "key": "user-agent",
            "value": "(?i).*(android|webos|iphone|ipad|ipod|blackberry|iemobile|opera mini).*"
          }
        ],
       
        "destination": "/api/debug"
      },
      {
        
        "source": "/(.*)",
        "destination": "/desktop/$1"
      }
    ]
  }
  